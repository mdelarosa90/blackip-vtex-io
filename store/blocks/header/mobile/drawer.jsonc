{
  "drawer#deparments": {
    "title": "Departamentos Drawer",
    "blocks": [
      "drawer-header#deparments"
    ],
    "children": [
      "flex-layout.row#deparments-container"
    ],
    "props": {
      "isFullWidth": true
    }
  },
  "flex-layout.row#deparments-container": {
    "children": [
      "flex-layout.col#deparments-container"
    ],
    "props": {
      "fullWidth": true,
      "blockClass": [
        "w-100"
      ]
    }
  },
  "flex-layout.col#deparments-container": {
    "children": [
      "drawer#department-1",
      "flex-layout.row#drawer-footer"
    ]
  },
  "drawer-header#deparments": {
    "title": "Header Departamentos Drawer",
    "children": [
      "drawer-close-button",
      "rich-text#deparments"
    ],
    "props": {
      "blockClass": [
        "header-drawer"
      ]
    }
  },
  "rich-text#deparments": {
    "title": "Departamentos Texto",
    "props": {
      "text": "Departamentos"
    }
  },
  "image#icon-menu": {
    "title": "Open Icon Menu Image",
    "props": {
      "src": "https://i.imgur.com/r1VYUKc.png",
      "height": 10,
      "width": 10
    }
  },
  "flex-layout.row#drawer-footer": {
    "children": [
      "login",
      "menu#menu-favorites"
    ],
    "props": {
      "preserveLayoutOnMobile": true,
      "preventHorizontalStretch": true,
      "horizontalAlign": "center"
    }
  },
  "menu#menu-favorites": {
    "children": [
      "menu-item#favorites"
    ]
  },
  "menu-item#favorites": {
    "props": {
      "type": "custom",
      "iconPosition": "left",
      "blockClass": [
        "favorites"
      ],
      "iconProps": {
        "id": "mpa-heart",
        "size": 24,
        "viewBox": "0 0 24 24"
      },
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "/wishlist",
        "noFollow": false,
        "tagTitle": "Favoritos",
        "text": "FAVORITOS"
      }
    }
  }
}