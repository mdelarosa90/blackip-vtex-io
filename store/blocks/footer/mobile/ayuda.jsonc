{
  "disclosure-layout#ayuda": {
    "title": "Disclosure Ayuda",
    "children": [
      "disclosure-trigger#ayuda",
      "disclosure-content#ayuda"
    ]
  },
  "disclosure-trigger#ayuda": {
    "children": ["rich-text#ayuda", "disclosure-state-indicator"],
    "props": {
      "blockClass": ["bgBlack", "indicator-white", "pl0"]
    }
  },
  "disclosure-content#ayuda": {
    "children": ["menu#ayuda"]
  },
  "rich-text#ayuda": {
    "title": "Texto ayuda",
    "props": {
      "text": "**Ayuda**",
      "blockClass": ["text-white", "text-18"]
    }
  },
  "menu#ayuda": {
    "title": "Menu ayuda",
    "props": {
      "orientation": "vertical",
      "blockClass": ["column", "bgBlack", "link-white", "ml0-d", "text-14"],
      "items": [
        {
          "id": "menu-item-ayuda-1",
          "type": "custom",
          "highlight": false,
          "itemProps": {
            "type": "internal",
            "href": "/preguntas-frecuentes",
            "noFollow": false,
            "text": "Preguntas frecuentes"
          }
        },
        {
          "id": "menu-item-ayuda-2",
          "type": "custom",
          "highlight": false,
          "itemProps": {
            "type": "internal",
            "href": "/politicas-de-pago",
            "noFollow": false,
            "text": "Políticas de pago"
          }
        },
        {
          "id": "menu-item-ayuda-3",
          "type": "custom",
          "highlight": false,
          "itemProps": {
            "type": "internal",
            "href": "/politicas-de-envio",
            "noFollow": false,
            "text": " Políticas de envío"
          }
        },
        {
          "id": "menu-item-ayuda-4",
          "type": "custom",
          "highlight": false,
          "itemProps": {
            "type": "internal",
            "href": "/condiciones-de-cambio",
            "noFollow": false,
            "text": "Condiciones de Cambio"
          }
        },
        {
          "id": "menu-item-ayuda-5",
          "type": "custom",
          "highlight": false,
          "itemProps": {
            "type": "internal",
            "href": "/politicas-de-garantia",
            "noFollow": false,
            "text": "Políticas de Garantías"
          }
        }
      ]
    }
  },
  "disclosure-state-indicator": {
    "props": {
      "Show": "icon#down",
      "Hide": "icon#up"
    }
  },
  "icon#down": {
    "props": {
      "id": "mpa-angle--down",
      "size": 20
    }
  },
  "icon#up": {
    "props": {
      "id": "mpa-angle--up",
      "size": 20
    }
  }
}