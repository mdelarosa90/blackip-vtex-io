{
  "flex-layout.row#banners": {
    "children": [
      "list-context.image-list#main-banner"
    ],
    "props": {
      "fullWidth": true
    }
  },
  "list-context.image-list#main-banner": {
    "children": ["slider-layout#main-banner-images"],
    "title": "Banners Principales",
    "props": {
      "preload": false,
      "height": 650,
      "images": [
        {
          "image": "https://i.imgur.com/oY7YEDM.jpeg",
          "description": "something something",
          "width": "100%",
          "link": {
            "url": "/category-1"
          }
        },
        {
          "image": "https://i.imgur.com/oY7YEDM.jpeg",
          "description": "something something",
          "width": "100%",
          "link": {
            "url": "/category-2"
          }
        },
        {
          "image": "https://i.imgur.com/oY7YEDM.jpeg",
          "description": "something something",
          "width": "100%",
          "link": {
            "url": "/category-3"
          }
        }
      ]
    }
  },

  "slider-layout#main-banner-images": {
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "infinite": true,
      "slideTransition": {
        "speed": 3500
      },
      "autoplay": {
        "timeout": 3500,
        "stopOnHover": false
      }
    },
    "children": [
      "link#video"
    ]
  },

  "link#video" : {
    "children": [
      "video#prueba"
    ],
    "props": {
      "href": "/video-prueba"
    }
  },
  "video#prueba": {
    "props": {
      "width": "500px",
      "height": "600px",
      "loop": false,
      "autoPlay": false,
      "muted": false,
      "playsInline": true,
      //"poster": "https://www.errepar.com/resources/NuevoPortalERREIUS/Img/NotaOpinion/760x320/210.jpg",
      "src": "https://blacksip.com/imgload/Forus-UY/videos/V%C3%ADdeo-intervenci%C3%B3n-botas.mp4",
      "blockClass": "videoEl"
    }
  }
}